{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "src/**/*", 
    "src/node_modules"
  ],
  "exclude": [
    "node_modules/*", 
    "__sapper__/*", 
    "public/*", 
    ".eslintrc.js"
  ],
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "importsNotUsedAsValues": "remove",
    "strict": true
  }
}
