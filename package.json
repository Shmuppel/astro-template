{
  "name": "astro-template",
  "version": "0.0.1",
  "type": "module",
  "private": false,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test:vitest": "vitest run",
    "test:vitest-watch": "vitest",
    "test:vitest-coverage": "vitest run --coverage",
    "test:vitest-coverage-ci": "vitest run --coverage.enabled --coverage.reporter='text-summary'",
    "test:playwright": "npx playwright test",
    "lint:css": "stylelint 'src/**/*.{astro,css,svelte}'",
    "lint:js": "prettier --check 'src/**/*.{js,ts,tsx,jsx,svelte,astro}' && eslint 'src/**/*.{js,ts,tsx,jsx,svelte,astro}'",
    "lint:svelte-check": "svelte-check"
  },
  "dependencies": {
    "@astrojs/svelte": "^1.0.2",
    "astro": "^1.6.15",
    "svelte": "^3.55.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitest/coverage-c8": "^0.25.8",
    "astro-compress": "^1.1.23",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.21.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.1",
    "prettier-plugin-astro": "^0.7.0",
    "prettier-plugin-svelte": "^2.9.0",
    "sass": "^1.57.0",
    "stylelint": "^14.16.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-order": "^5.0.0",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^5.0.0",
    "vitest": "^0.25.8"
  }
